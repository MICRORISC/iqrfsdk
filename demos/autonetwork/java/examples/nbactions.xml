<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback.xml -classpath %classpath com.microrisc.simply.iqrf.dpa.v22x.autonetwork.demo.AutoNetworkDemo -discoveryTxPower 7 -prebondingInterval 15 -autoUseFrc true</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback.xml -classpath %classpath com.microrisc.simply.iqrf.dpa.v22x.autonetwork.demo.AutoNetworkDemo -discoveryTxPower 7 -prebondingInterval 15 -autoUseFrc true</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Djava.library.path=src/main/resources/natives/x64 -Dlogback.configurationFile=config/logback.xml -classpath %classpath com.microrisc.simply.iqrf.dpa.v22x.autonetwork.demo.AutoNetworkDemo -discoveryTxPower 7 -prebondingInterval 15 -autoUseFrc true</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
